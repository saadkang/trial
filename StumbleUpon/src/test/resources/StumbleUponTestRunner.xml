<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="StumbleUpon" parallel="tests">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="firefox"/>
    <parameter name="browserVersion" value="53.0"/>
    <parameter name="url" value="https://stumbleupon.com"/>

    <test name = "StumbleUpon Home">
        <classes>
            <class name="login.LoginPage"/>
            <class name="testpageurl.TestPageUrl"/>
        </classes>
    </test>
</suite>